{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/home", "dest": "/api/home.js" },
    { "src": "/api/homePost", "dest": "/api/homePost.js" },
    { "src": "/api/homeDelete", "dest": "/api/homeDelete.js" }
  ]
}
